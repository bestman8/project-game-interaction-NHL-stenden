<Window x:Class="project_1_game_inteact.Leaderboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Leaderboard" Height="350" Width="525">
    <Grid>
        <DataGrid x:Name="LeaderboardDataGrid" AutoGenerateColumns="False" CanUserAddRows="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name" Binding="{Binding Name}" Width="*"/>
                <DataGridTextColumn Header="Points" Binding="{Binding Points}" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
